        android:layout_toRightOf="@+id/Sixes2"
        android:src="@drawable/dice_6" />

    <ImageButton
        android:id="@+id/RolledDice2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/RolledDice1"
