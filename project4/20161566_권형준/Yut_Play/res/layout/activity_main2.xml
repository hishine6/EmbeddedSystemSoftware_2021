        android:layout_below="@+id/smallstraight2"
        android:gravity="center"
        android:layout_marginTop="2dp"
        android:textSize="19sp" />

    <TextView
        android:id="@+id/yacht1"
        android:layout_width="63dp"
        android:layout_height="22dp"
