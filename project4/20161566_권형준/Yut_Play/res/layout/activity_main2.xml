        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/RolledDice3"
        android:layout_alignTop="@+id/SavedDice2"
        android:src="@drawable/empty" />

    <ImageButton
        android:id="@+id/SavedDice4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
