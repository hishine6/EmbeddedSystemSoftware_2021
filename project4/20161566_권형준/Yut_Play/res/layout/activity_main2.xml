        android:textSize="19sp" />

    <TextView
        android:id="@+id/Bonus1"
        android:layout_width="63dp"
        android:layout_height="22dp"
        android:layout_alignLeft="@+id/Sixes1"
        android:layout_below="@+id/Sixes1"
        android:layout_marginTop="17dp"
        android:gravity="center"
        android:textSize="19sp" />

    <TextView
        android:id="@+id/Bonus2"
        android:layout_width="63dp"
        android:layout_height="22dp"
        android:layout_alignLeft="@+id/Sixes2"
        android:layout_below="@+id/Sixes2"
        android:layout_marginTop="17dp"
        android:gravity="center"
        android:textSize="19sp" />

    <TextView
        android:id="@+id/Subtotal1"
        android:layout_width="63dp"
        android:layout_height="22dp"
        android:layout_alignLeft="@+id/Aces1"
        android:layout_below="@+id/Sixes1"
