    <TextView
        android:id="@+id/Fours2"
        android:layout_width="63dp"
        android:layout_height="22dp"
        android:layout_alignLeft="@+id/Aces2"
        android:layout_below="@+id/Threes2"
        android:layout_marginTop="2dp"
        android:gravity="center"
        android:textSize="19sp" />

    <TextView
        android:id="@+id/Fives1"
        android:layout_width="63dp"
        android:layout_height="22dp"
        android:layout_alignLeft="@+id/Aces1"
        android:layout_below="@+id/Fours1"
