        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/SavedDice1"
        android:layout_alignLeft="@+id/Aces1"
        android:layout_alignRight="@+id/Aces1"
        android:gravity="center"
        android:text="1"
        android:textSize="50sp" />

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView01"
        android:layout_alignBottom="@+id/TextView01"
        android:layout_toLeftOf="@+id/SavedDice1"
        android:layout_toRightOf="@+id/total1"
        android:gravity="center"
        android:text="2"
        android:textSize="50sp" />

    <TextView
        android:id="@+id/playtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/SavedDice1"
        android:layout_alignTop="@+id/total2"
        android:gravity="center"
        android:textSize="30sp" />

</RelativeLayout>